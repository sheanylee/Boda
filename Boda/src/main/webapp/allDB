--탈퇴 회원 테이블7
drop table d_member;
create table d_member(
	no number primary key,
	name varchar2(12) not null,
	id varchar2(12) not null,
	pw varchar2(12) not null,
	repw varchar2(12) not null,
	ph0 varchar2(10),
	ph1 varchar2(10),
	ph2 varchar2(10),
	email varchar2(20),
	point number default 0,
	time varchar2(30)
);




--store9

drop table store cascade constraints;
create table store(
   s_num number primary key,
   s_code varchar2(20),
   s_image varchar2(30),
   s_name varchar2(30) not null,
   s_price varchar2(10) not null,
   s_stock varchar2(10) not null,
   s_content varchar2(80),
   s_point varchar2(10) default 0
);

alter table s_orders modify o_name varchar2(30);

drop sequence s_seq;
create sequence s_seq
increment by 1
start with 1
minvalue 1
maxvalue 1000
nocache;

