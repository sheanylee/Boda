drop sequence review_seq;
create sequence review_seq start with 1 increment by 1 nocache;

drop table review cascade constraints;
create table review(
	r_num number primary key, --관람평 번호
	r_memberId varchar2(30) not null, --회원 아이디
	r_movieName varchar2(100) not null, --영화 이름
	r_star number, --관람평 평점
	r_review varchar2(4000), --관람평 내용
	r_date varchar2(100) --관람평 작성일
);

