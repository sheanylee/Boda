col r_num for 99
col r_memberId for a15
col r_movieNum for a30
col r_star for 99
col r_review for a100
col r_date for a10
select * from review;

-----------------------------
drop sequence movie_seq;
create sequence movie_seq start with 1 increment by 1 nocache;

drop table movie cascade constraints;
create table movie(
   m_num number primary key, --번호
   m_code varchar2(50), --코드
   m_name varchar2(1000), --이름
   m_genre varchar2(500), --장르
   m_img varchar2(100), --포스터
   m_grade varchar2(50), --시청등급
   m_nation varchar2(50), --제작국가
   m_star number, --평점
   m_pd varchar2(50), --감독
   m_actor varchar2(1000), --배우
   m_runtime number, --러닝타임
   m_per number, --예매율
   m_date date, --개봉일
   m_enddate date, --종영일
   m_review varchar2(4000), --리뷰
   m_info varchar2(4000), --정보
   m_view number, --누적관객
   m_like number default 0 --좋아요
);

insert into movie values(
   movie_seq.nextval, 
   '2022KOR001', 
   '한산: 용의 출현', 
   '드라마, 액션', 
   'movie_image_한산.jpg', 
   '12', 
   '한국', 
   '9.2', 
   '김한민', 
   '박해일, 변요한, 안성기, 손현주, 김성규, 김성균, 김향기, 옥택연, 공명, 박지환, 조재윤', 
   '129', 
   '0.2',
   '2022/07/27', 
   '2022/12/27', 
   '익히 아는 전투의 결과, 하지만 그곳까지 몰입하여 끌려가도록 하는 눈을 떼기 어려운 작품!!!', 
   '나라의 운명을 바꿀 압도적 승리의 전투가 시작된다!
1592년 4월, 조선은 임진왜란 발발 후 단 15일 만에 왜군에 한양을 빼앗기며 절체절명의 위기에 놓인다.
조선을 단숨에 점령한 왜군은 명나라로 향하는 야망을 꿈꾸며 대규모 병역을 부산포로 집결시킨다.
한편, 이순신 장군은 연이은 전쟁의 패배와 선조마저 의주로 파천하며 수세에 몰린 상황에서도
조선을 구하기 위해 전술을 고민하며 출전을 준비한다.
