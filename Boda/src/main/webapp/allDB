--book13

drop sequence bookseq;
create sequence bookseq
increment by 1
start with 1
nocache;


drop table book cascade constraints;
create table book(
   r_num number primary key, --seq
   r_userId varchar2(30) not null,
   r_price number,
   r_reserDay date not null,
   r_mname varchar2(30) not null,
   r_theater varchar2(30) not null,
   r_cinema number,
   r_day date not null,
   r_mstart varchar2(20) not null,
   r_sno varchar2(30) not null,
   r_paying varchar2(30) not null,
   r_point number default 0,
   r_ticket varchar2(50)
);


