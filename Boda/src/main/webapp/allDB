--s_orders;
drop table s_orders cascade constraints;
create table s_orders(
   o_num number,
   s_num number,
   o_id varchar2(15),
   o_name varchar2(30),
   o_order number, --주문 수량
   o_price number,
   o_total_price number,
   o_total_point number default 0
);
drop sequence o_seq;
create sequence o_seq
increment by 1
start with 1
minvalue 1
maxvalue 1000
nocache;

commit;


--영화관람권 테이블
drop table myticket cascade constraints;
create table myticket(
	t_id varchar2(20),
	t_spec varchar2(30),
	t_order number
);

col t_id for a8
col t_spec for a15
col t_order for 999
select * from myticket;
commit;

--에러나서 데이터 크기 변경할게요 이렇게만 sql plus에 써주시면 돼요
alter table s_orders modify o_name varchar2(30);
--indemovie12


drop table indeMovie cascade constraints;
create table indeMovie( --총 19
num number primary key,
kind varchar2(20), --단편, 장편, 개막작
title varchar2(30), --영화제목
img varchar2(60), --영화 메인 사진
director varchar2(50), --감독
year varchar2(5),
genre varchar2(20),
color varchar2(10) default 'color',
type varchar2(20) default 'DCP',
runtime varchar2(50),
premiere varchar2(20) default '', --world premier 유무
synopsis varchar2(500),
intention varchar2(500),
fest_awards varchar2(300), --수상경력
direc_img1 varchar2(30), --감독1
direc_info1 varchar2(200), --감독1 경력
direc_img2 varchar2(30), --감독2
direc_info2 varchar2(200), --감독2 경력
note varchar2(2000),
video varchar2(100),
likes number default 0
);

drop sequence inde_seq;
create sequence inde_seq
increment by 1
start with 1
nocache
;

