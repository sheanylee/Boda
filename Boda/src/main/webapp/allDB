--year income

drop sequence income_seq;
create sequence income_seq
increment by 1
start with 1
nocache;

drop table income cascade constraints;
create table income(
no number primary key,
t_name varchar2(50) unique, 
y2019 number default 0,
y2020 number default 0,
y2021 number default 0,
y2022 number default 0
);

insert into income values(income_seq.nextval, 'Boda강남' ,200000000,100000000,120000000,150000000);
insert into income values(income_seq.nextval, 'Boda신촌' ,180000000,80000000,100000000,150000000);

commit;

---visit[방문자수 (통계)]

drop sequence visit_seq;
create sequence visit_seq
increment by 1
start with 1
minvalue 1
nocache;

DROP TABLE visit CASCADE CONSTRAINTS;
create table visit( 
	vno number primary key, --pk
	visit_date date default sysdate, --방문날짜
	cnt number default 0 --몇번
);


