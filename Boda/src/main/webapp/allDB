--timetable1
drop sequence time_seq;
create sequence time_seq
increment by 1
start with 1
nocache;

drop table timetable cascade constraints;

create table timetable( --12
num number primary key,
theater varchar2(20) not null,
cinema number not null, -- 󿵰 
day timestamp not null,
mstart timestamp default sysdate,
gap number default 15,
mend timestamp default sysdate,
mname varchar2(80) not null,
mgenre varchar2(15),
mgrade number,
mrun number,
open date,
bookingseat number default 0
);

insert into timetable (num ,theater ,cinema ,day ,mstart ,mend ,mname) 
