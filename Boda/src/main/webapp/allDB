--member6

drop sequence m_seq;

create sequence m_seq   
increment by 1
start with 1
minvalue 1
maxvalue 10000
nocache;

drop table member cascade constraints;
create table member(
	no number primary key,
	name varchar2(12) not null,
	id varchar2(12) not null,
	pw varchar2(12) not null,
	repw varchar2(12) not null,
	ph0 varchar2(10),
	ph1 varchar2(10),
	ph2 varchar2(10),
	email varchar2(20),
	point number default 0
);

insert into member values(0, '관리자', 'admin', '0000', '0000', '010', '1234', '1234', 'admin@naver.com',0);
