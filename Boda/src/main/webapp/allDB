--seat3
--seat
drop table seat cascade constraints;
create table seat(
	sno varchar2(30), --seat number
	scode number, --80,100,150
	userNo varchar2(30), --id
	status number not null, --0,1
	tnum number, --timetable num
    foreign key(tnum) references timetable(num)
);



