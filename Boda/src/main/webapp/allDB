----------event DB

drop table event cascade constraints;
create table event(
	no number,
	title varchar2(80),
	contents varchar2(70),
	main_img varchar2(50),
	contents_img varchar2(50),
	sday varchar2(25),
	eday varchar2(25)
);

drop sequence e_seq;
create sequence e_seq
increment by 1
start with 1
nocache;

col no for 99
col title for a8
col contents for a8
col main_img for a12
col contents_img for a12
col sday for a12
col eday for a12
select * from event;

commit;


[room]
	
--room table

drop sequence room_seq;
create sequence room_seq
	increment by 1
	start with 10
	minvalue 1
	maxvalue 10000000
	nocache;
	
drop table room cascade constraints;
create table room(
	no number default 0,
	room_no number primary key,
	mno1 number references member(no) on delete cascade,
	mno2 number references member(no) on delete cascade,
	mno1_lastvisit timestamp default '70/01/01 00:00:00.000000',
	mno2_lastvisit timestamp default '70/01/01 00:00:00.000000'
	);


insert into room values(0, 1, 1,0,'22/11/23 14:11:42.000000','22/11/23 14:06:22.000000');
insert into room values(0, 2, 2,0,'22/11/23 13:56:15.000000','22/11/23 14:06:20.000000');
insert into room values(0, 3, 3,0,'22/11/23 14:12:47.000000','22/11/23 14:13:14.000000');
insert into room values(0, 4, 4,0,'22/11/23 14:14:40.000000','22/11/23 14:15:11.000000');
insert into room values(0, 5, 5,0,'22/11/23 14:16:48.000000','22/11/23 14:17:06.000000');
insert into room values(0, 6, 6,0,'22/11/23 14:18:40.000000','70/01/01 00:00:00.000000');
insert into room values(0, 7, 7,0,'70/01/01 00:00:00.000000','70/01/01 00:00:00.000000');
insert into room values(0, 8, 8,0,'70/01/01 00:00:00.000000','70/01/01 00:00:00.000000');

col room_no for 99
col mno1 for 99
col mno2 for 99
col MNO1_LASTVISIT for a25
col MNO2_LASTVISIT for a25
select * from room;




--chat table

drop sequence chat_seq;
create sequence chat_seq
	increment by 1
	start with 26
	minvalue 1
	maxvalue 10000000
	nocache;


drop table chat cascade constraints;
create table chat(
	no number primary key,
	room_no number references room(room_no) on delete cascade,
	send_mno number references member(no) on delete cascade,
	read_mno number references member(no) on delete cascade,
	time timestamp default sysdate,
	content varchar2(1000) not null
	);


